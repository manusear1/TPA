using System;
class MainClass {
  public static void Main (string[] args) {
    int[] vetor = new int [15];

    for(int i = 0; i<15; i++){
    Console.Write($"Digite o {i+1}Â° valor:");
    vetor[i] = int.Parse(Console.ReadLine());
    Console.Clear();
    }

    for(int x = 0; x<15; x++){
      if(x%2==0){
        Console.WriteLine(vetor[x]);
      }
    }
  }
}
